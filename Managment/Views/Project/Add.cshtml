@using Management.Common.Enum;@using Management.Common.Models.DTO;@model Management.Common.Models.DTO.ProjectDto;<head>    <link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/habibmhamadi/multi-select-tag@2.0.1/dist/css/multi-select-tag.css"></head><style>    .bootstrap-tagsinput {        min-height: 100px;    }</style>@{    ViewBag.Statuses = new SelectList(Enum.GetValues(typeof(Status)));}@{    Layout = "_accountLayout";}<h1 style="margin-left: 301px">Create New Project Here</h1><div class="container">    <form asp-action="AddProject" method="post" enctype="multipart/form-data">        <div asp-validation-summary="ModelOnly" class="text-danger"></div>        <div class="form-group">            <label asp-for="Logo"></label>            <br />            <input class="btn btn-secondary" type="file" asp-for="Logo" />        </div>        <div class="form-group">            <label asp-for="Name"></label>            <input asp-for="Name" type="text" name="Name" id="Name" class="form-control" placeholder="Name of the Project" />            <span asp-validation-for="Name" class="text-danger"></span>        </div>        <div class="form-group">            <label asp-for="StartDate"></label>            <input type="date" name="StartDate" asp-for="StartDate" class="form-control" />            <span asp-validation-for="StartDate" class="text-danger"></span>        </div>        <label asp-for="Status "></label>        @Html.DropDownList("Statuses", null, htmlAttributes: new { @class = "form-control" })        <span asp-validation-for="Status" class="text-danger"></span>        <div class="form-group">            <label asp-for="Description "></label>            <input asp-for="Description " class="form-control" />            <span asp-validation-for="Description " class="text-danger"></span>        </div>        <label asp-for="TechStackUsed">TechStackUsed</label>        <select asp-for="TechStackUsed" asp-items="@ViewBag.TechStackNames" class="form-control" id="TechStackUsed" multiple>        </select>        <div class="form-group">            <label asp-for="Documentation"></label>            <br />            <input class="btn btn-secondary" type="file" asp-for="Documentation" />        </div>        <div class="form-group">            <label asp-for="SnapShoots"></label>            <br />            <input class="btn btn-secondary" type="file" asp-for="SnapShoots" multiple />        </div>        <div class="form-group">            <label asp-for="DevelopmentName  "></label>            <input asp-for="DevelopmentName  " class="form-control" />            <span asp-validation-for="DevelopmentName  " class="text-danger"></span>        </div>        <div class="form-group">            <label asp-for="DevelopmentUrl  "></label>            <input asp-for="DevelopmentUrl  " class="form-control" />            <span asp-validation-for="DevelopmentUrl  " class="text-danger"></span>        </div>        <div class="form-group">            <label asp-for="StageName  "></label>            <input asp-for="StageName  " class="form-control" />            <span asp-validation-for="StageName  " class="text-danger"></span>        </div>        <div class="form-group">            <label asp-for="StageUrl   "></label>            <input asp-for="StageUrl   " class="form-control" />            <span asp-validation-for="StageUrl   " class="text-danger"></span>        </div>        <div class="form-group">            <label asp-for="ProductionName   "></label>            <input asp-for="ProductionName   " class="form-control" />            <span asp-validation-for="ProductionName   " class="text-danger"></span>        </div>        <div class="form-group">            <label asp-for="ProductionUrl   "></label>            <input asp-for="ProductionUrl   " class="form-control" />            <span asp-validation-for="ProductionUrl   " class="text-danger"></span>        </div>        <br />        <button type="submit" class="btn btn-primary">Save</button>    </form></div><script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>@* <script src="../../assets/vendor/bootstrap-tagsinput/js/bootstrap-tagsinput.min.js"></script><script src="../js/bootstrap-tagsinput.js"></script> *@<script src="https://cdn.jsdelivr.net/gh/habibmhamadi/multi-select-tag@2.0.1/dist/js/multi-select-tag.js"></script><script>    new MultiSelectTag('TechStackUsed')</script>